# Local Development Environment Variables
# Copy this file to .env and fill in your values

# Pushover notification settings (required for both local and deployed)
PUSHOVER_TOKEN=your_pushover_token_here
PUSHOVER_USER=your_pushover_user_here

# Redis configuration (required for both local and deployed)
# For local development, you can run: redis-server
# Or use Docker: docker run -d -p 6379:6379 redis:alpine
REDIS_URL=redis://localhost:6379

# Browserless configuration (only needed for deployed environment)
# Leave these empty for local development
BROWSER_TOKEN=
BROWSER_WEBDRIVER_ENDPOINT=

# Booking credentials (only needed if using book_on_time.py)
USERNAME=your_email@example.com
PASSWORD=your_password_here
